{
  "compilerOptions": {
    "moduleResolution": "bundler",
    "lib": ["es2023", "dom"],
    "types": ["vite/client"],
    "module": "es2022",
    "target": "es2023",
    "resolveJsonModule": true,
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "strict": true,
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "skipLibCheck": true
  }
}
